  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "eslint:recommended"
    ],
    "rules": {
      "no-empty-function": "error",
      "no-empty-pattern": "error",
      "no-multi-spaces": "error",
      "no-multi-str": "error",
      "require-await": "error",
      "array-bracket-spacing": [
        "error",
        "never"
      ],
      "block-spacing": "error",
      "brace-style": "error",
      "comma-spacing": [
        "error",
        {
          "before": false,
          "after": true
        }
      ],
      "comma-style": [
        "error",
        "last"
      ],
      "func-call-spacing": [
        "error",
        "never"
      ],
      "indent": [
        "error",
        2
      ],
      "key-spacing": [
        "error",
        {
          "beforeColon": false
        }
      ],
      "keyword-spacing": [
        "error",
        {
          "before": true
        }
      ],
      "max-depth": [
        "error",
        4
      ],
      "max-len": [
        "error",
        {
          "code": 100,
          "tabWidth": 2
        }
      ],
      "semi": [
        "error",
        "always"
      ],
      "semi-spacing": [
        "error",
        {
          "before": false,
          "after": true
        }
      ],
      "semi-style": [
        "error",
        "last"
      ],
      "no-undef": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.4",
    "postcss": "^8.4.12",
    "tailwindcss": "^3.0.24"
  }
}
